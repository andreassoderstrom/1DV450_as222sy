<h1> Nytt projekt! </h1>

<%= form_for @project do |p| %>
	<%= p.label :name %>
	<%= p.text_field :name %> 
	<%= p.label :description %>
	<%= p.text_field :description %>
	<%= p.label :start_date %>
	<%= p.text_field :start_date %>
	<%= p.label :end_date %>
	<%= p.text_field :end_date %>
	<%= p.submit "Skapa projekt" %>

<% end %>

<%= link_to "Tillbaka", projects_path %>